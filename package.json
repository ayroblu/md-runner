{
  "name": "md-runner",
  "version": "1.0.0",
  "description": "Runs markdown code blocks",
  "main": "src/index.ts",
  "type": "module",
  "license": "NONE",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.11.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "start": "NODE_OPTIONS=\"--experimental-json-modules --no-warnings\" ts-node-esm src/index.ts",
    "start:node-repl": "ts-node-esm src/node-repl.ts",
    "build": "tsc",
    "test": "echo \"Todo\"",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier src -w",
    "ci": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@types/lodash": "^4.14.191",
    "commander": "^9.4.1",
    "lodash": "^4.17.21",
    "rehype-add-classes": "^1.0.0",
    "rehype-document": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-pretty-code": "^0.5.1",
    "rehype-prism-plus": "^1.5.0",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "rehype-shiki": "^0.0.9",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "remark-toc": "^8.0.1",
    "shiki": "^0.11.1",
    "unified": "^10.1.2",
    "uuid": "^9.0.0"
  }
}
